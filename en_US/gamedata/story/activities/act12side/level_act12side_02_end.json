            }
        },
        {
            "id": 123,
            "prop": "name",
            "attributes": {
                "content": "A very persuasive verdict. Let's split up.",
                "name": "Lin Yühsia"
            },
            "figure_art": "avg_npc_196_1#5"
        },
        {
            "id": 124,
