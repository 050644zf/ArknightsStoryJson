        },
        {
            "id": 162,
            "prop": "name",
            "attributes": {
                "content": "   There's too many people. Hey, I'm walkin' here!",
                "name": "Mafioso"
            },
            "figure_art": "avg_npc_031"
        },
        {
            "id": 163,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_031",
