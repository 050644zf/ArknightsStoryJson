        {
            "id": 170,
            "prop": "name",
            "attributes": {
                "content": "   ...I don't think they saw us.",
                "name": "Bison"
            },
            "figure_art": "char_325_bison_1#4"
        },
        {
            "id": 171,
            "prop": "Character",
            "attributes": {
                "name": "char_325_bison_1#4",
                "name2": "char_213_mostma_1",
                "focus": 2.0
            }
        },
        {
            "id": 172,
            "prop": "name",
            "attributes": {
                "content": "   Just keep going like this, and we'll figure out a way to meet back up with the others.",
                "name": "Mostima"
            },
            "figure_art": "char_213_mostma_1"
        },
        {
            "id": 173,
            "prop": "Character",
            "attributes": {
                "name": "char_325_bison_1#4",
                "name2": "char_213_mostma_1",
                "focus": 1.0
            }
        },
        {
            "id": 174,
            "prop": "name",
            "attributes": {
                "content": "   Right.",
                "name": "Bison"
            },
            "figure_art": "char_325_bison_1#4"
        },
        {
            "id": 175,
            "prop": "Character",
            "attributes": {
                "name": "char_325_bison_1#4",
