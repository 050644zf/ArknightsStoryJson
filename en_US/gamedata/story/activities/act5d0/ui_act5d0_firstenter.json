            }
        },
        {
            "id": 20,
            "prop": "name",
            "attributes": {
                "content": "   You win again. But you deal slower than I do with my toes.",
                "name": "Gambino"
            },
            "figure_art": "avg_npc_028#4"
        },
        {
            "id": 21,
            "prop": "PlayMusic",
            "attributes": {
                "intro": "$bar_intro",
