        {
            "id": 15,
            "prop": "name",
            "attributes": {
                "content": "   Jack of Spades. Straight flush. I win again, Gambino.",
                "name": "Capone"
            },
            "figure_art": "avg_npc_027#3"
        },
        {
            "id": 16,
            "prop": "Dialog",
            "attributes": {}
        },
        {
            "id": 17,
