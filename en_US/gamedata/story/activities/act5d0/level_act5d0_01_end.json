        },
        {
            "id": 92,
            "prop": "Character",
            "attributes": {
                "name": "char_102_texas_1#2",
                "name2": "char_105_emper",
                "focus": 1.0
            }
        },
        {
            "id": 93,
            "prop": "name",
            "attributes": {
