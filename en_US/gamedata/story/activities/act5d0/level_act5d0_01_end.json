        },
        {
            "id": 99,
            "prop": "Character",
            "attributes": {
                "name": "char_325_bison_1#2",
                "name2": "char_103_angel_1#7",
                "focus": 2.0
            }
        },
        {
            "id": 100,
            "prop": "name",
            "attributes": {
                "content": "   Huh. ",
                "name": "Exusiai"
            }
        },
        {
            "id": 101,
            "prop": "Character",
            "attributes": {
                "name": "char_102_texas_1"
            }
        },
        {
            "id": 102,
            "prop": "name",
            "attributes": {
                "content": "   We're a logistics company. Why are we always getting involved in gang wars?",
