            }
        },
        {
            "id": 90,
            "prop": "Character",
            "attributes": {
                "name": "char_102_texas_1",
                "name2": "char_105_emper",
                "focus": 2.0
            }
        },
        {
            "id": 91,
            "prop": "name",
            "attributes": {
                "content": "   Sigh. Texas! Go scout 'em out!",
