        },
        {
            "id": 42,
            "prop": "Character",
            "attributes": {
                "name": "char_103_angel_1#8",
                "name2": "char_201_moeshd#3",
                "focus": 2.0
            }
        },
        {
            "id": 43,
            "prop": "name",
            "attributes": {
                "content": "   Ya know she'll come a-runnin' soon as Texas gets on the horn.",
                "name": "Croissant"
            }
        },
        {
            "id": 44,
            "prop": "Character",
            "attributes": {
                "name": "char_201_moeshd"
            }
        },
        {
            "id": 45,
            "prop": "name",
