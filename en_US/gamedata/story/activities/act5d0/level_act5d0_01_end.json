        },
        {
            "id": 38,
            "prop": "Character",
            "attributes": {
                "name": "char_102_texas_1",
                "name2": "char_105_emper",
                "focus": 2.0
            }
        },
        {
            "id": 39,
            "prop": "name",
            "attributes": {
                "content": "   What's for dinner?",
