        {
            "id": 9,
            "prop": "name",
            "attributes": {
                "content": "   ...Would you be able to walk around a bit with me, Doctor?",
                "name": "Ceylon"
            },
            "figure_art": "char_348_ceylon_4#9"
        },
        {
            "id": 10,
            "prop": "Decision",
            "attributes": {
                "options": "I'm a little worried about you.",
                "values": "1"
            },
            "targetLine": {
