        },
        {
            "id": 19,
            "prop": "name",
            "attributes": {
                "content": "   The guests are all here, and they will take the stage in 2 minutes and 30 seconds.",
                "name": "Staff"
            },
            "figure_art": "avg_npc_020"
        },
        {
            "id": 20,
            "prop": "name",
            "attributes": {
                "content": "   Quick! Take care of whatever is going on outside.",
                "name": "Staff"
            },
            "figure_art": "avg_npc_020"
        },
        {
            "id": 21,
