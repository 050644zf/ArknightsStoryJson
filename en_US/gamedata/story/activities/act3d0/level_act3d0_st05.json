        },
        {
            "id": 136,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_024"
            }
        },
        {
            "id": 137,
            "prop": "name",
            "attributes": {
                "content": "   Don't mind me. This is between you youngsters.",
                "name": "Herman"
            }
        },
        {
            "id": 138,
