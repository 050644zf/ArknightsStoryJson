        },
        {
            "id": 147,
            "prop": "name",
            "attributes": {
                "content": "   Don't worry, I'm not your enemy anymore.",
                "name": "Schwarz"
            }
        },
        {
            "id": 148,
            "prop": "Predicate",
            "attributes": {
                "references": "2"
            },
