        {
            "id": 153,
            "prop": "name",
            "attributes": {
                "content": "  My apologies, Sir Vincent.",
                "name": "Kal'tsit"
            },
            "figure_art": "avg_npc_170_1#3"
        },
        {
            "id": 154,
            "prop": "name",
            "attributes": {
                "content": "  Might I have arrived late?",
                "name": "Kal'tsit"
            },
            "figure_art": "avg_npc_170_1#3"
