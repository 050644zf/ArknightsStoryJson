                "name": "avg_npc_169_1#2"
            }
        },
        {
            "id": 66,
            "prop": "CameraShake",
            "attributes": {
                "duration": 0.5,
                "xstrength": 15.0,
                "ystrength": 15.0,
                "vibrato": 20.0,
