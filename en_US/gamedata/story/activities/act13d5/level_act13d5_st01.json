                "focus": 2.0
            }
        },
        {
            "id": 194,
            "prop": "name",
            "attributes": {
                "content": "  Yes, Sir Szewczyk.",
                "name": "Corporate Employee"
            },
            "figure_art": "avg_npc_109"
        },
        {
            "id": 195,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_103",
