        {
            "id": 192,
            "prop": "name",
            "attributes": {
                "content": "  Hey, let me see the match card for the day.",
                "name": "Plastic Knight"
            },
            "figure_art": "avg_npc_103"
        },
        {
            "id": 193,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_103",
