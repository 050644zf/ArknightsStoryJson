            "attributes": {
                "name": "avg_npc_121"
            }
        },
        {
            "id": 162,
            "prop": "name",
            "attributes": {
                "content": "  Today's match is fully sponsored by the Roar Guards Company, as usual. The top ten competitors in the daily points rankings will each receive one limited edition weapon 'Rioter' provided by the Roar Guards!",
                "name": "Greatmouth Mob"
            },
            "figure_art": "avg_npc_121"
        },
