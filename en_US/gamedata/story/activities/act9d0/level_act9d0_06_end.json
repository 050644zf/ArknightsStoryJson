        }
    },
    {
        "id": 72,
        "prop": "name",
        "attributes": {
            "content": "  Hoederer, yes?",
            "name": "Confessarius"
        }
    },
    {
        "id": 73,
        "prop": "name",
        "attributes": {
            "content": "  I look forward to seeing what else you uncover.",
            "name": "Confessarius"
        }
    },
    {
        "id": 74,
        "prop": "Dialog",
        "attributes": {}
    },
    {
        "id": 75,
        "prop": "Character",
