                "time": 0.7
            }
        },
        {
            "id": 153,
            "prop": "PlaySound",
            "attributes": {
                "key": "$d_gen_walk_n",
                "volume": 1.0
            }
        },
        {
            "id": 154,
            "prop": "Character",
