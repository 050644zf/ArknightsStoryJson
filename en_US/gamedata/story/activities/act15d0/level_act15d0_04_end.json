        {
            "id": 68,
            "prop": "Character",
            "attributes": {
                "name": "char_108_silent_1#3",
                "name2": "char_empty"
            }
        },
        {
            "id": 69,
            "prop": "Delay",
            "attributes": {
                "time": 2.0
            }
