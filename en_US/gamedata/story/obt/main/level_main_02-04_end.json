        },
        {
            "id": 6,
            "prop": "name",
            "attributes": {
                "content": "  You were right... This bunch came prepared!",
                "name": "Franka"
            },
            "figure_art": "char_106_franka_1"
        },
        {
            "id": 7,
            "prop": "name",
            "attributes": {
                "content": "  You guys should pick up the pace too!",
                "name": "Franka"
            },
            "figure_art": "char_106_franka_1"
        },
        {
            "id": 8,
            "prop": "Character",
