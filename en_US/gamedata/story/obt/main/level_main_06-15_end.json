        {
            "id": 63,
            "prop": "Decision",
            "attributes": {
                "options": "What about the burden you bear?!;........;Didn't you promise to join Rhodes Island?",
                "values": "1;2;3"
            },
            "targetLine": {
                "option1": "line64",
                "option2": "line64",
                "option3": "line64"
            }
        },
        {
            "id": 64,
            "prop": "Predicate",
            "attributes": {
                "references": "1;2;3"
