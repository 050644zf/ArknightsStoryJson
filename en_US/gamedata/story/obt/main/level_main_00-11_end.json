        {
            "id": 86,
            "prop": "Character",
            "attributes": {
                "name": "char_1508_Faust_1",
                "fadetime": 2.0,
                "block": "true"
            }
        },
        {
            "id": 87,
            "prop": "name",
            "attributes": {
                "content": "   Sorry... I failed.",
                "name": "Faust"
            },
            "figure_art": "char_1508_Faust_1"
        },
        {
            "id": 88,
            "prop": "Character",
            "attributes": {
                "name": "char_1507_Mephisto_1#1",
                "name2": "char_1508_Faust_1",
                "focus": 1.0
            }
        },
        {
            "id": 89,
            "prop": "name",
            "attributes": {
                "content": "  No, don't apologize. It was my mistake. I lost my temper.",
                "name": "Mephisto"
            },
            "figure_art": "char_1507_Mephisto_1#1"
        },
        {
            "id": 90,
            "prop": "name",
            "attributes": {
                "content": "  Can you help me track Rhodes Island? I'll report the situation to Talulah first.",
                "name": "Mephisto"
            },
            "figure_art": "char_1507_Mephisto_1#1"
        },
        {
            "id": 91,
            "prop": "name",
            "attributes": {
                "content": "  She should have taken over Chernobog's core command tower by now.",
                "name": "Mephisto"
            },
            "figure_art": "char_1507_Mephisto_1#1"
        },
        {
            "id": 92,
            "prop": "name",
            "attributes": {
                "content": "  ...She will decide the fate of these insects.",
                "name": "Mephisto"
            },
            "figure_art": "char_1507_Mephisto_1#1"
        },
        {
            "id": 93,
            "prop": "Character",
            "attributes": {
                "name": "char_1507_Mephisto_1#1",
                "name2": "char_1508_Faust_1",
                "focus": 2.0
            }
        },
        {
            "id": 94,
            "prop": "name",
            "attributes": {
                "content": "   ...Understood.",
                "name": "Faust"
            },
            "figure_art": "char_1508_Faust_1"
        },
        {
            "id": 95,
            "prop": "Character",
            "attributes": {
                "name": "char_1507_Mephisto_1#2",
                "name2": "char_1508_Faust_1",
                "focus": 1.0
            }
        },
        {
            "id": 96,
            "prop": "name",
            "attributes": {
                "content": "  Be careful. Your safety is the most important thing, all right?",
                "name": "Mephisto"
            },
            "figure_art": "char_1507_Mephisto_1#2"
        },
        {
            "id": 97,
            "prop": "Character",
            "attributes": {
                "name": "char_1507_Mephisto_1#1",
                "name2": "char_1508_Faust_1",
                "focus": 2.0
            }
        },
        {
            "id": 98,
            "prop": "name",
            "attributes": {
                "content": "   ...I will.",
                "name": "Faust"
            },
            "figure_art": "char_1508_Faust_1"
        },
        {
            "id": 99,
            "prop": "Blocker",
            "attributes": {
                "a": 1.0,
