        "attributes": {
            "time": 0.6
        }
    },
    {
        "id": 75,
        "prop": "name",
        "attributes": {
            "content": "   But, I cannot do these things...",
            "name": "Ch'en"
        }
    },
    {
        "id": 76,
        "prop": "name",
        "attributes": {
            "content": "   And especially... not the L.G.D.",
            "name": "Ch'en"
        }
    },
    {
        "id": 77,
        "prop": "Character",
        "attributes": {
            "name": "char_010_chen_1",
            "name2": "char_002_amiya_1#4",
            "focus": 2.0
