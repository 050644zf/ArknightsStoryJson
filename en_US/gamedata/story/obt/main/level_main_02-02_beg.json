                "a": 0.6,
                "r": 1.0,
                "g": 1.0,
                "b": 1.0,
                "fadetime": 0.1,
                "block": "true"
            }
        },
        {
            "id": 96,
            "prop": "Blocker",
            "attributes": {
                "a": 0.0,
                "fadetime": 0.1,
                "block": "true"
            }
        },
        {
            "id": 97,
            "prop": "CameraShake",
            "attributes": {
                "duration": 0.5,
                "xstrength": 10.0,
                "ystrength": 12.0,
                "vibrato": 15.0,
                "randomness": 90.0,
                "fadeout": "true",
                "block": "false"
            }
        },
        {
            "id": 98,
            "prop": "name",
            "attributes": {
                "content": "   Your good intentions are not lost upon us despite your rudeness.",
                "name": "Ch'en"
            },
            "figure_art": "char_010_chen_1#5"
        },
        {
            "id": 99,
            "prop": "name",
            "attributes": {
                "content": "   However, we also are not a charity for useless lobbyists.",
                "name": "Ch'en"
            },
            "figure_art": "char_010_chen_1#5"
        },
        {
            "id": 100,
            "prop": "Character",
            "attributes": {
                "name": "char_2005_weiyw_1",
                "name2": "char_010_chen_1#5",
                "focus": 1.0
            }
        },
        {
            "id": 101,
            "prop": "name",
            "attributes": {
                "content": "   Madam Ch'en.",
                "name": "Wei Yenwu"
            },
