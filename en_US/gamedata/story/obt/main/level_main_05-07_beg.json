        {
            "id": 86,
            "prop": "Character",
            "attributes": {
                "name": "char_136_hsguma",
                "name2": "char_010_chen_1#5",
                "focus": 2.0
            }
        },
        {
            "id": 87,
            "prop": "name",
            "attributes": {
