        {
            "id": 27,
            "prop": "Character",
            "attributes": {
                "name": "char_308_swire_1#5",
                "name2": "char_010_chen_1#4",
                "focus": 2.0
            }
        },
        {
            "id": 28,
            "prop": "name",
            "attributes": {
