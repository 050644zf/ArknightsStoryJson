            "figure_art": "char_010_chen_1#5"
        },
        {
            "id": 33,
            "prop": "Character",
            "attributes": {
                "name": "char_002_amiya_1",
                "name2": "char_010_chen_1#5",
                "focus": 1.0
            }
        },
        {
