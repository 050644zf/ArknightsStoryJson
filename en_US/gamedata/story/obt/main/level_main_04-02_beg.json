            }
        },
        {
            "id": 149,
            "prop": "Character",
            "attributes": {
                "name": "char_219_meteo_1",
                "name2": "char_193_frostl_1",
                "focus": 1.0
            }
        },
        {
            "id": 150,
            "prop": "name",
            "attributes": {
                "content": "   ...The walls of the ruins?",
