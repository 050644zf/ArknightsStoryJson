        },
        {
            "id": 49,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_026"
            }
        },
        {
            "id": 50,
            "prop": "name",
            "attributes": {
                "content": "  Forgive me. We are mere moments from undertaking a mission. I must focus my energy on calibrating my weaponry.",
                "name": "Scout"
