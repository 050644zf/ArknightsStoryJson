            }
        },
        {
            "id": 30,
            "prop": "Character",
            "attributes": {
                "name": "char_002_amiya_1#2"
            }
        },
        {
            "id": 31,
            "prop": "name",
            "attributes": {
                "content": "   Doctor, are you feeling better now?",
                "name": "Amiya"
            }
        },
        {
            "id": 32,
            "prop": "Character",
            "attributes": {
                "name": "char_002_amiya_1#4"
            }
        },
        {
            "id": 33,
            "prop": "name",
            "attributes": {
                "content": "   You looked like you were still in pain and and couldn't move around well yesterday.",
                "name": "Amiya"
            }
        },
        {
            "id": 34,
            "prop": "Decision",
            "attributes": {
                "options": "I've made a complete recovery. How about you?",
                "values": "1"
            },
            "targetLine": {
                "option1": "line35"
            }
        },
        {
            "id": 35,
            "prop": "Predicate",
            "attributes": {
                "references": "1"
