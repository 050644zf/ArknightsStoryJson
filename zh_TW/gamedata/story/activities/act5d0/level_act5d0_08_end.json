        },
        {
            "id": 89,
            "prop": "Character",
            "attributes": {
                "name": "char_213_mostma_1#3",
                "name2": "avg_npc_034#3",
                "focus": 1.0
            }
        },
        {
            "id": 90,
            "prop": "name",
            "attributes": {
                "content": "   先把那家糖果鋪買下來。",
