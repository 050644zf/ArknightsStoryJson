            }
        },
        {
            "id": 192,
            "prop": "name",
            "attributes": {
                "content": "   欸？",
                "name": "能天使"
            }
        },
        {
            "id": 193,
            "prop": "Character",
            "attributes": {
                "name": "char_102_texas_1#2"
            }
        },
        {
            "id": 194,
            "prop": "name",
            "attributes": {
                "content": "   唉，趴下。",
                "name": "德克薩斯"
            }
