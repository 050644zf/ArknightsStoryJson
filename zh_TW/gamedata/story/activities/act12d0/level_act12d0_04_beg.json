            "attributes": {
                "name": "char_187_ccheal_1",
                "name2": "char_411_tomimi_1#3",
                "focus": 1.0
            }
        },
        {
            "id": 175,
            "prop": "name",
            "attributes": {
                "content": "  然後？",
                "name": "嘉維爾"
            }
        },
        {
            "id": 176,
            "prop": "Character",
            "attributes": {
                "name": "char_187_ccheal_1"
            }
        },
        {
            "id": 177,
