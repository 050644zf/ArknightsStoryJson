            "attributes": {
                "name": "char_136_hsguma",
                "name2": "char_010_chen_1#1",
                "focus": 2.0
            }
        },
        {
            "id": 32,
            "prop": "name",
            "attributes": {
                "content": "  很危險。",
                "name": "陳"
