            "id": 75,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_079#5",
                "name2": "avg_npc_078#2",
                "focus": 2.0
            }
        },
        {
            "id": 76,
            "prop": "name",
            "attributes": {
