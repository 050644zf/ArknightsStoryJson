        },
        {
            "id": 58,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_079",
                "name2": "avg_npc_078#2",
                "focus": 2.0
            }
        },
        {
            "id": 59,
            "prop": "name",
            "attributes": {
