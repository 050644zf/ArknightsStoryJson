        },
        {
            "id": 64,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_042",
                "name2": "char_002_amiya_1#4",
                "focus": 1.0
            }
        },
        {
            "id": 65,
            "prop": "name",
            "attributes": {
                "content": " 不然呢，不然呢！我難道不能恨她嗎？",
                "name": "迷迭香"
            }
        },
        {
            "id": 66,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_042",
                "name2": "char_002_amiya_1#4",
                "focus": 2.0
            }
        },
        {
            "id": 67,
            "prop": "name",
            "attributes": {
                "content": " 迷迭香......",
