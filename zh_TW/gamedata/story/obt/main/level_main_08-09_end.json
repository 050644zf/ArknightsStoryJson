        },
        {
            "id": 70,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_042",
                "name2": "char_002_amiya_1#4",
                "focus": 1.0
            }
        },
        {
            "id": 71,
            "prop": "name",
            "attributes": {
                "content": " 命令？",
