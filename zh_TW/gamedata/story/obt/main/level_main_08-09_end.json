            }
        },
        {
            "id": 62,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_042",
                "name2": "char_002_amiya_1#4",
                "focus": 2.0
            }
        },
        {
            "id": 63,
            "prop": "name",
            "attributes": {
                "content": " 你恨塔露拉。",
