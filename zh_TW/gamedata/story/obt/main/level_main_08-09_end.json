        },
        {
            "id": 41,
            "prop": "Character",
            "attributes": {
                "name": "char_002_amiya_1#7",
                "name2": "avg_npc_058",
                "focus": 1.0
            }
        },
        {
            "id": 42,
            "prop": "name",
            "attributes": {
