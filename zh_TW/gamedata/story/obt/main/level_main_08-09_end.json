            "prop": "Character",
            "attributes": {
                "name": "avg_npc_042",
                "name2": "char_002_amiya_1",
                "focus": 2.0
            }
        },
        {
            "id": 73,
            "prop": "name",
            "attributes": {
                "content": " 是。",
