        },
        {
            "id": 121,
            "prop": "Character",
            "attributes": {
                "name": "char_012_misa_1#4",
                "name2": "char_1500_skulsr",
                "focus": 1.0
            }
        },
        {
            "id": 122,
            "prop": "name",
            "attributes": {
                "content": "   我，我不知道......",
