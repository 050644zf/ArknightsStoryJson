        {
            "id": 207,
            "prop": "Character",
            "attributes": {
                "name": "char_1507_Mephisto_1#1",
                "name2": "char_1508_Faust_1#1",
                "focus": 1.0
            }
        },
        {
            "id": 208,
            "prop": "name",
            "attributes": {
