        },
        {
            "id": 25,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_080",
                "name2": "avg_npc_078#2",
                "focus": 2.0
            }
        },
        {
            "id": 26,
            "prop": "name",
            "attributes": {
