                "focus": 2.0
            }
        },
        {
            "id": 9,
            "prop": "name",
            "attributes": {
                "content": "  止血了沒？",
                "name": "陳"
            }
        },
        {
            "id": 10,
            "prop": "Character",
            "attributes": {
                "name": "char_136_hsguma",
                "name2": "char_010_chen_1#1",
                "focus": 1.0
