        },
        {
            "id": 31,
            "prop": "Character",
            "attributes": {
                "name": "char_002_amiya_1"
            }
        },
        {
            "id": 32,
            "prop": "name",
            "attributes": {
                "content": "   我會小心的。",
                "name": "阿米婭"
            }
        },
        {
            "id": 33,
            "prop": "Character",
            "attributes": {
                "name": "char_002_amiya_1"
            }
        },
        {
            "id": 34,
