        {
            "id": 142,
            "prop": "Character",
            "attributes": {
                "name": "char_187_ccheal_1",
                "name2": "avg_npc_075",
                "focus": 1.0
            }
        },
        {
            "id": 143,
            "prop": "name",
            "attributes": {
                "content": "  글쎄, 다른 일이 있는 것 같더라고. 나중에 다시 합류할 거야.",
                "name": "가비알"
