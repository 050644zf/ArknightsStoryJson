        },
        {
            "id": 75,
            "prop": "Decision",
            "attributes": {
                "options": "그건 무모함이 아니다.;나는 많은 일은 하지 않았다.;당신이 감사해야 할 사람은 실론이다.",
                "values": "1;2;3"
            },
            "targetLine": {
                "option1": "line76",
                "option2": "line76",
                "option3": "line76"
            }
        },
        {
            "id": 76,
            "prop": "Predicate",
