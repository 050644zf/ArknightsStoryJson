        },
        {
            "id": 42,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_052",
                "name2": "avg_npc_047",
                "focus": 2.0
            }
        },
        {
            "id": 43,
            "prop": "name",
            "attributes": {
