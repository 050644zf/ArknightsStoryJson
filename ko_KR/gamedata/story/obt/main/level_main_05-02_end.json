        },
        {
            "id": 72,
            "prop": "Character",
            "attributes": {
                "name": "char_136_hsguma",
                "name2": "char_010_chen_1#1",
                "focus": 2.0
            }
        },
        {
            "id": 73,
            "prop": "name",
            "attributes": {
                "content": "  마음대로 추측해도 된다.",
                "name": "첸"
