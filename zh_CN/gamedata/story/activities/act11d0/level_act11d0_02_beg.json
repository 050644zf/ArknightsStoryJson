        {
            "id": 107,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_069",
                "name2": "char_345_folnic_1#4",
                "focus": 2.0
            }
        },
        {
            "id": 108,
            "prop": "name",
            "attributes": {
                "content": "  不过现在因为某些原因，这里也是抗议者最多的地方。",
                "name": "亚叶"
            },
            "figure_art": "char_345_folnic_1#4"
