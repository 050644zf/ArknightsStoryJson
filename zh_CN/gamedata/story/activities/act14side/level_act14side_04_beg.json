        },
        {
            "id": 185,
            "prop": "name",
            "attributes": {
                "content": "爸，咱们给老爷送碗水去吧。",
                "name": "谢拉格平民C"
            },
            "figure_art": "avg_npc_280_1#1$1"
        },
