            }
        },
        {
            "id": 141,
            "prop": "name",
            "attributes": {
                "content": "  喂，博士，别赌气不听我说话嘛！",
                "name": "嘉维尔"
            },
            "figure_art": "char_187_ccheal_1#2"
        },
        {
            "id": 142,
            "prop": "Predicate",
