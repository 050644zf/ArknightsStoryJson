                "name": "卡彭"
            }
        },
        {
            "id": 120,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_027#2",
                "name2": "avg_npc_031",
                "focus": 2.0
            }
        },
        {
            "id": 121,
            "prop": "name",
            "attributes": {
                "content": "   对、对不起，追！都给我去追！",
                "name": "黑帮"
            }
        },
        {
            "id": 122,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_027#2"
            }
        },
        {
            "id": 123,
            "prop": "name",
            "attributes": {
                "content": "   哼......",
                "name": "卡彭"
            }
        },
        {
            "id": 124,
            "prop": "Dialog",
            "attributes": {}
        },
        {
            "id": 125,
            "prop": "Character",
            "attributes": {}
        },
        {
            "id": 126,
