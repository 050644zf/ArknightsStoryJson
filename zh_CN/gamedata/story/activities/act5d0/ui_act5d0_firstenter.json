                "name2": "avg_npc_027#2",
                "focus": 2.0
            }
        },
        {
            "id": 57,
            "prop": "name",
            "attributes": {
                "content": "   彼此彼此，首领。",
                "name": "卡彭"
            },
            "figure_art": "avg_npc_027#2"
