            },
            "endOfOpt": false
        },
        {
            "id": 123,
            "prop": "name",
            "attributes": {
                "content": "  很少有不会被迷迭香的战斗震慑住的人。",
                "name": "凯尔希"
            },
            "figure_art": "char_003_kalts_1"
        },
        {
            "id": 124,
            "prop": "name",
            "attributes": {
                "content": "  做好心理准备。",
                "name": "凯尔希"
            },
            "figure_art": "char_003_kalts_1"
