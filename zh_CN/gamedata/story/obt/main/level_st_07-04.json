        {
            "id": 67,
            "prop": "name",
            "attributes": {
                "content": "  赦罪师，回答。",
                "name": "？？？"
            }
        },
        {
            "id": 68,
            "prop": "name",
            "attributes": {
                "content": "  整合运动的领袖，是否有结盟的价值？",
                "name": "？？？"
            }
        },
        {
            "id": 69,
            "prop": "Dialog",
            "attributes": {}
        },
        {
            "id": 70,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_055",
                "fadetime": 1.0,
                "block": "true"
            }
        },
        {
