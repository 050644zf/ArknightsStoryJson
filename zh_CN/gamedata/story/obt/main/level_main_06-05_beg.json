        {
            "id": 167,
            "prop": "name",
            "attributes": {
                "content": "  喂——！",
                "name": "诗怀雅"
            }
        },
        {
            "id": 168,
            "prop": "Character",
            "attributes": {
                "name": "char_010_chen_1#2"
            }
        },
        {
            "id": 169,
            "prop": "name",
            "attributes": {
                "content": "  诗怀雅，别过来！",
                "name": "陈"
            }
        },
        {
            "id": 170,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_038",
