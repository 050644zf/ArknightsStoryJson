        },
        {
            "id": 65,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_037"
            }
        },
        {
            "id": 66,
            "prop": "name",
            "attributes": {
                "content": "  我没意见。你们队长怎么说？",
                "name": "雪怪小队成员A"
            }
        },
        {
            "id": 67,
            "prop": "Character",
            "attributes": {
                "name": "char_002_amiya_1#7"
            }
        },
        {
            "id": 68,
            "prop": "name",
            "attributes": {
                "content": "  ......",
                "name": "阿米娅"
            }
        },
        {
            "id": 69,
            "prop": "Character",
            "attributes": {
