            }
        },
        {
            "id": 7,
            "prop": "name",
            "attributes": {
                "content": "   あそこにいるのは――",
                "name": "アーミヤ"
            }
        },
        {
            "id": 8,
            "prop": "name",
            "attributes": {
                "content": "   チェン隊長！",
                "name": "アーミヤ"
            }
        },
        {
            "id": 9,
