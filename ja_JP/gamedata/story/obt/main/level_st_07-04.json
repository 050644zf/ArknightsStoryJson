        {
            "id": 67,
            "prop": "name",
            "attributes": {
                "content": "  聴罪師、答えよ。",
                "name": "？？？"
            }
        },
        {
            "id": 68,
            "prop": "name",
            "attributes": {
                "content": "  レユニオン・ムーブメントのリーダーには、同盟を結ぶ価値があるか否か。",
                "name": "？？？"
            }
        },
        {
            "id": 69,
            "prop": "Dialog",
            "attributes": {}
        },
        {
            "id": 70,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_055",
