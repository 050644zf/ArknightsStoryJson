        },
        {
            "id": 11,
            "prop": "name",
            "attributes": {
                "content": "   お二人とも、これ以上は……。",
                "name": "ホシグマ"
            },
            "figure_art": "char_010_chen_1#5"
        },
        {
            "id": 12,
            "prop": "Character",
