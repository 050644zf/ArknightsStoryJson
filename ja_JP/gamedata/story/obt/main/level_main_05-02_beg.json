            "id": 174,
            "prop": "Character",
            "attributes": {
                "name": "char_010_chen_1#1",
                "name2": "avg_npc_012",
                "focus": 1.0
            }
        },
        {
            "id": 175,
            "prop": "name",
            "attributes": {
                "content": "   まだそのような要求は出していないはずだが。",
                "name": "チェン"
