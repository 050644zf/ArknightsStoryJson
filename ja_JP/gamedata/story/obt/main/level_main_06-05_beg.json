                "focus": 1.0
            }
        },
        {
            "id": 113,
            "prop": "name",
            "attributes": {
                "content": "  追ってくるべきではなかったな。",
                "name": "？？？"
            },
            "figure_art": "avg_npc_038"
        },
        {
            "id": 114,
            "prop": "name",
            "attributes": {
                "content": "  部下を黙らせろ、チェン。",
                "name": "？？？"
            },
            "figure_art": "avg_npc_038"
        },
        {
            "id": 115,
            "prop": "Character",
            "attributes": {
                "name": "char_010_chen_1#5"
            }
        },
        {
            "id": 116,
            "prop": "name",
            "attributes": {
                "content": "  ……。",
                "name": "チェン"
            },
            "figure_art": "char_010_chen_1#5"
        },
        {
            "id": 117,
            "prop": "name",
            "attributes": {
                "content": "  お前たちは、スワイヤーのところへ向かえ。",
                "name": "チェン"
            },
            "figure_art": "char_010_chen_1#5"
        },
        {
            "id": 118,
            "prop": "Character",
            "attributes": {
                "name": "char_010_chen_1#5",
                "name2": "char_015_lmg",
                "focus": 2.0
            }
        },
        {
            "id": 119,
            "prop": "name",
            "attributes": {
                "content": "  しかしチェン隊長……。",
                "name": "近衛局隊員"
            },
            "figure_art": "char_015_lmg"
        },
        {
            "id": 120,
