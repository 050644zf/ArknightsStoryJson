        {
            "id": 181,
            "prop": "name",
            "attributes": {
                "content": "  クソッ！！",
                "name": "チェン"
            },
            "figure_art": "char_010_chen_1#2"
        },
        {
            "id": 182,
            "prop": "Dialog",
            "attributes": {}
        },
        {
            "id": 183,
