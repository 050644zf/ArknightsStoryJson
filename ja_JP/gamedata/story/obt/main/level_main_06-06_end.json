            }
        },
        {
            "id": 173,
            "prop": "name",
            "attributes": {
                "content": "  自己紹介が必要でしょうか？",
                "name": "？？？"
            },
            "figure_art": "avg_npc_041"
        },
        {
            "id": 174,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_041",
                "name2": "char_367_swllow_1",
                "focus": 2.0
            }
        },
        {
            "id": 175,
            "prop": "name",
            "attributes": {
                "content": "  いらない。",
                "name": "グレースロート"
            },
            "figure_art": "char_367_swllow_1"
        },
        {
            "id": 176,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_041",
                "name2": "char_367_swllow_1",
                "focus": 1.0
            }
        },
        {
            "id": 177,
            "prop": "name",
            "attributes": {
                "content": "  感染者と友人になるのは危険です。",
                "name": "？？？"
            },
            "figure_art": "avg_npc_041"
        },
        {
            "id": 178,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_041",
                "name2": "char_367_swllow_1",
                "focus": 2.0
            }
        },
        {
            "id": 179,
            "prop": "name",
            "attributes": {
                "content": "  私は友人が感染者かどうかは気にしない。",
                "name": "グレースロート"
            },
            "figure_art": "char_367_swllow_1"
        },
        {
            "id": 180,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_041",
                "name2": "char_367_swllow_1",
                "focus": 1.0
            }
        },
        {
            "id": 181,
            "prop": "name",
            "attributes": {
                "content": "  そうですか……。",
                "name": "？？？"
            },
            "figure_art": "avg_npc_041"
        },
        {
            "id": 182,
            "prop": "name",
            "attributes": {
