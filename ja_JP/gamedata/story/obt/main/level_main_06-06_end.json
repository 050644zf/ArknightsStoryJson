        {
            "id": 97,
            "prop": "name",
            "attributes": {
                "content": "  君さ、今それを聞く？",
                "name": "ブレイズ"
            },
            "figure_art": "char_017_homura_3#9"
        },
        {
            "id": 98,
            "prop": "Character",
