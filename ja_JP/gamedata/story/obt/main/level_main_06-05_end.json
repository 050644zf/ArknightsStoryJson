            }
        },
        {
            "id": 157,
            "prop": "Character",
            "attributes": {
                "name": "char_308_swire_1#6"
            }
        },
        {
            "id": 158,
            "prop": "name",
            "attributes": {
                "content": "  えっ？",
                "name": "スワイヤー"
            }
        },
        {
            "id": 159,
            "prop": "name",
            "attributes": {
                "content": "  もしもし？　もしもし！？　ネズ公！！",
                "name": "スワイヤー"
            }
        },
        {
            "id": 160,
            "prop": "name",
            "attributes": {
                "content": "  *龍門スラング*！　*龍門スラング*！　*龍門スラング*！　よくも切ったわね！！　あたしの電話を！！",
                "name": "スワイヤー"
            }
        },
