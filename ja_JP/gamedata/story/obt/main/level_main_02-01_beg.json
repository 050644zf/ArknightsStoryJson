            }
        },
        {
            "id": 31,
            "prop": "name",
            "attributes": {
                "content": "   身体の具合はどうですか？",
                "name": "アーミヤ"
            }
        },
        {
            "id": 32,
            "prop": "Character",
            "attributes": {
                "name": "char_002_amiya_1#4"
            }
        },
        {
            "id": 33,
            "prop": "name",
            "attributes": {
                "content": "   どこか痛いですか？　昨日はまだ動き辛そうでしたから…。",
                "name": "アーミヤ"
            }
        },
        {
            "id": 34,
            "prop": "Decision",
            "attributes": {
                "options": "もう大丈夫だ。アーミヤは？",
                "values": "1"
            },
            "targetLine": {
