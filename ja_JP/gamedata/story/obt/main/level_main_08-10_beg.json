            "attributes": {
                "name": "avg_npc_058",
                "name2": "avg_npc_078",
                "focus": 1.0
            }
        },
        {
            "id": 156,
            "prop": "name",
            "attributes": {
                "content": "  西ではすでに監視隊が活動を開始している……奴らに連中の足跡を発見させるわけにはいかない。",
                "name": "盾兵"
            },
            "figure_art": "avg_npc_058"
        },
