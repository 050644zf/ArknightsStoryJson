        {
            "id": 104,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_079",
                "name2": "avg_npc_078#2",
                "focus": 1.0
            }
        },
        {
            "id": 105,
            "prop": "name",
            "attributes": {
                "content": "  でも私……",
                "name": "アリーナ"
            },
            "figure_art": "avg_npc_079"
        },
        {
            "id": 106,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_079",
                "name2": "avg_npc_078#2",
                "focus": 2.0
            }
        },
        {
            "id": 107,
            "prop": "name",
            "attributes": {
                "content": "  知識を武器にする者も戦士だ、アリーナ。この考えは貫くさ。",
