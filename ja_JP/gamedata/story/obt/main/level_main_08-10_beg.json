        {
            "id": 149,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_058",
                "name2": "avg_npc_078",
                "focus": 2.0
            }
        },
        {
            "id": 150,
            "prop": "name",
            "attributes": {
                "content": "  わかった。あまり厳しい言葉は使うなよ。過度に責め立てるな。",
                "name": "タルラ"
            },
            "figure_art": "avg_npc_078"
        },
