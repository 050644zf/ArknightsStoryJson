            "attributes": {
                "name": "char_003_kalts_1",
                "name2": "char_002_amiya_1#5",
                "focus": 2.0
            }
        },
        {
            "id": 143,
            "prop": "name",
            "attributes": {
                "content": "   う～……",
                "name": "アーミヤ"
            },
            "figure_art": "char_002_amiya_1#5"
        },
        {
            "id": 144,
            "prop": "Character",
            "attributes": {
                "name": "char_003_kalts_1",
                "name2": "char_002_amiya_1#4",
                "focus": 2.0
            }
        },
        {
            "id": 145,
            "prop": "name",
            "attributes": {
                "content": "   ケルシー先生、あまりドクターを困らせないでくださいね。",
                "name": "アーミヤ"
            },
            "figure_art": "char_002_amiya_1#4"
        },
        {
            "id": 146,
            "prop": "Character",
            "attributes": {
                "name": "char_003_kalts_1",
                "name2": "char_002_amiya_1#4",
                "focus": 1.0
            }
        },
        {
            "id": 147,
