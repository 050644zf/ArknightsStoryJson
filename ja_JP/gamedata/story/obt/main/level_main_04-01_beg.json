            }
        },
        {
            "id": 140,
            "prop": "Character",
            "attributes": {
                "name": "char_003_kalts_1",
                "name2": "char_002_amiya_1#3",
                "focus": 1.0
            }
        },
        {
            "id": 141,
            "prop": "name",
            "attributes": {
                "content": "   ダメだ。しかもそれでも聞こえるだろう。先に行っていてくれ。",
                "name": "ケルシー"
