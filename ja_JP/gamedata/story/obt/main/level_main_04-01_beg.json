        },
        {
            "id": 133,
            "prop": "name",
            "attributes": {
                "content": "   ――",
                "name": "アーミヤ"
            }
        },
        {
            "id": 134,
            "prop": "Character",
            "attributes": {
                "name": "char_003_kalts_1",
                "name2": "char_002_amiya_1#2",
                "focus": 2.0
            }
        },
        {
            "id": 135,
            "prop": "name",
            "attributes": {
                "content": "   私も居ていいですか？",
                "name": "アーミヤ"
