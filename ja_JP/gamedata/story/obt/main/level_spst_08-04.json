        {
            "id": 121,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_012"
            }
        },
        {
            "id": 122,
            "prop": "name",
            "attributes": {
                "content": " ……あんまり感性は豊かな方じゃない俺でも、あれを見せられた後だと、言いたいことが伝わってくるぜ。",
                "name": "疲弊したオペレーター"
            },
            "figure_art": "avg_npc_012"
        },
        {
            "id": 123,
            "prop": "Character",
            "attributes": {
                "name": "avg_npc_088"
            }
        },
        {
            "id": 124,
            "prop": "name",
            "attributes": {
                "content": " あれは、レユニオンのリーダーの――",
                "name": "冷静なオペレーター"
