        {
            "id": 182,
            "prop": "name",
            "attributes": {
                "content": " 私たちはもうかなり、石棺の中心に近づいている。",
                "name": "ケルシー"
            },
            "figure_art": "char_003_kalts_1#2"
        },
        {
            "id": 183,
            "prop": "name",
            "attributes": {
                "content": " ……",
                "name": "ケルシー"
            },
            "figure_art": "char_003_kalts_1#2"
        },
        {
            "id": 184,
            "prop": "Decision",
            "attributes": {
                "options": "何を心配している？",
                "values": "1"
            },
            "targetLine": {
                "option1": "line185"
            }
        },
        {
            "id": 185,
