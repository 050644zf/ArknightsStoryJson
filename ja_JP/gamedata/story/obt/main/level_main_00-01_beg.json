        {
            "id": 122,
            "prop": "name",
            "attributes": {
                "content": "  ふっ。……頼んだぞ。",
                "name": "ドーベルマン"
            },
            "figure_art": "char_130_doberm_ex"
        },
        {
            "id": 123,
            "prop": "name",
            "attributes": {
                "content": "  では、アーミヤ。号令を。",
                "name": "ドーベルマン"
