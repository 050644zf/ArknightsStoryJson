        {
            "id": 154,
            "prop": "Decision",
            "attributes": {
                "options": "いい感じだ！;……。;彼女たちの関係、少しは和らいだ？",
                "values": "1;2;3"
            },
            "targetLine": {
                "option1": "line155",
