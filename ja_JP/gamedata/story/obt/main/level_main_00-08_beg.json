        {
            "id": 106,
            "prop": "Character",
            "attributes": {
                "name": "char_014_riope"
            }
        },
        {
            "id": 107,
            "prop": "name",
            "attributes": {
                "content": "    こっちの準備はできてる！　だが、無理やり突破するにしても――",
                "name": "Ace"
