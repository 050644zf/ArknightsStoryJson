        },
        {
            "id": 116,
            "prop": "Character",
            "attributes": {
                "name": "char_002_amiya_1#7"
            }
        },
        {
            "id": 117,
            "prop": "name",
            "attributes": {
                "content": "   はい。できるだけ敵の制圧もお願いします。レユニオンに反撃の機会を与えないようにしたいです。",
                "name": "アーミヤ"
