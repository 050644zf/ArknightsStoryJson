        },
        {
            "id": 82,
            "prop": "Character",
            "attributes": {
                "name": "char_103_angel_1",
                "name2": "char_107_liskam_1",
                "focus": 2.0
            }
        },
        {
            "id": 83,
            "prop": "name",
            "attributes": {
