        },
        {
            "id": 104,
            "prop": "Character",
            "attributes": {
                "name": "char_1508_Faust_1#1",
                "name2": "char_367_swllow_1",
                "focus": 1.0
            }
        },
        {
            "id": 105,
            "prop": "name",
            "attributes": {
                "content": "  それなら、覚えておけ。",
                "name": "ファウスト"
            }
        },
        {
            "id": 106,
            "prop": "Character",
            "attributes": {
                "name": "char_1508_Faust_1#1",
                "name2": "char_367_swllow_1#4",
                "focus": 2.0
