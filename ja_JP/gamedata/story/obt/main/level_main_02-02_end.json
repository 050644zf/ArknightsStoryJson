        },
        {
            "id": 49,
            "prop": "Character",
            "attributes": {
                "name": "char_2005_weiyw_1",
                "name2": "char_010_chen_1",
                "focus": 1.0
            }
        },
        {
            "id": 50,
            "prop": "name",
            "attributes": {
                "content": "   ああ、そうだ。タルラだ。",
                "name": "ウェイ"
            }
        },
        {
            "id": 51,
            "prop": "name",
            "attributes": {
                "content": "   チェン隊長、君はどうだ？",
                "name": "ウェイ"
            }
        },
        {
            "id": 52,
            "prop": "Character",
            "attributes": {
                "name": "char_2005_weiyw_1",
                "name2": "char_010_chen_1#5",
                "focus": 2.0
            }
        },
        {
            "id": 53,
            "prop": "name",
            "attributes": {
                "content": "   ――――",
                "name": "チェン"
            }
        },
        {
            "id": 54,
            "prop": "name",
            "attributes": {
                "content": "   はい…私も――存じております。",
                "name": "チェン"
            }
        },
        {
            "id": 55,
            "prop": "Dialog",
            "attributes": {}
        },
        {
            "id": 56,
            "prop": "Blocker",
            "attributes": {
                "a": 1.0,
