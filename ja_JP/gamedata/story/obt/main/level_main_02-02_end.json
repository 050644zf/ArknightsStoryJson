        },
        {
            "id": 30,
            "prop": "name",
            "attributes": {
                "content": "   聞かせてもらおう。",
                "name": "ウェイ"
            },
            "figure_art": "char_2005_weiyw_1"
        },
        {
            "id": 31,
            "prop": "Character",
