        },
        {
            "id": 7,
            "prop": "name",
            "attributes": {
                "content": "   なるほど。君の評価は？",
                "name": "ウェイ"
            },
            "figure_art": "char_2005_weiyw_1"
        },
        {
            "id": 8,
