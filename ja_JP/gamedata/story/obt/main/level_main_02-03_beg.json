        {
            "id": 117,
            "prop": "name",
            "attributes": {
                "content": "   はい。……と、すみません、通信中ですので……",
                "name": "リスカム"
            },
            "figure_art": "char_107_liskam_1"
        },
        {
            "id": 118,
            "prop": "name",
            "attributes": {
