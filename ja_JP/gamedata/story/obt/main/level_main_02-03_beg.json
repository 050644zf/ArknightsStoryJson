        },
        {
            "id": 114,
            "prop": "Character",
            "attributes": {
                "name": "char_002_amiya_1",
                "name2": "char_107_liskam_1",
                "focus": 1.0
            }
        },
        {
            "id": 115,
            "prop": "name",
            "attributes": {
                "content": "   もしかして、チェン隊長ですか？",
                "name": "アーミヤ"
