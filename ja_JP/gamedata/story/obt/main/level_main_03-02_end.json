        },
        {
            "id": 143,
            "prop": "name",
            "attributes": {
                "content": "   それは違うよ……。",
                "name": "ミーシャ"
            }
        },
        {
            "id": 144,
            "prop": "Character",
            "attributes": {
                "name": "char_012_misa_1#4",
                "name2": "char_1500_skulsr",
                "focus": 2.0
            }
        },
        {
            "id": 145,
            "prop": "name",
            "attributes": {
                "content": "   お前にはまだ真実が見えていない。だがそろそろ、真実を受け入れて欲しい……。",
                "name": "スカルシュレッダー"
            }
        },
        {
            "id": 146,
            "prop": "name",
            "attributes": {
                "content": "   ……。",
                "name": "スカルシュレッダー"
            }
        },
        {
            "id": 147,
            "prop": "name",
            "attributes": {
                "content": "   ……すまん。ついカッとなってしまった。",
                "name": "スカルシュレッダー"
            }
        },
        {
            "id": 148,
            "prop": "Character",
            "attributes": {
                "name": "char_012_misa_1#4",
                "name2": "char_1500_skulsr",
                "focus": 1.0
            }
        },
        {
            "id": 149,
            "prop": "name",
            "attributes": {
                "content": "   大丈夫……私……。",
                "name": "ミーシャ"
            }
        },
        {
            "id": 150,
            "prop": "name",
            "attributes": {
                "content": "   ……私もわかるから……。",
                "name": "ミーシャ"
            }
        },
        {
            "id": 151,
            "prop": "name",
            "attributes": {
                "content": "   ずっと長い間、苦しんで……きたんだよね？",
