        },
        {
            "id": 153,
            "prop": "name",
            "attributes": {
                "content": "   ああ、みんなそうだ。",
                "name": "スカルシュレッダー"
            }
        },
        {
            "id": 154,
            "prop": "name",
            "attributes": {
                "content": "   だが今は、少なくとも全員が希望を抱いている。",
                "name": "スカルシュレッダー"
            }
        },
        {
            "id": 155,
            "prop": "name",
            "attributes": {
                "content": "   タルラ……あいつが感染者を率いて、残酷な全てと戦ってくれる。",
                "name": "スカルシュレッダー"
            }
        },
        {
            "id": 156,
            "prop": "name",
            "attributes": {
                "content": "   レユニオンが俺たちの希望なんだ。",
                "name": "スカルシュレッダー"
            }
        },
        {
            "id": 157,
            "prop": "Character",
            "attributes": {
                "name": "char_012_misa_1#4",
                "name2": "char_1500_skulsr",
                "focus": 1.0
            }
        },
        {
            "id": 158,
            "prop": "name",
            "attributes": {
                "content": "   ……やっぱりみんな、苦しい思いをしてるんだね。",
                "name": "ミーシャ"
            }
        },
        {
            "id": 159,
            "prop": "name",
            "attributes": {
                "content": "   もしレユニオンがみんなを保護してくれるなら……。",
                "name": "ミーシャ"
            }
        },
        {
            "id": 160,
            "prop": "Character",
            "attributes": {
                "name": "char_012_misa_1#4",
                "name2": "char_1500_skulsr",
                "focus": 2.0
            }
        },
        {
            "id": 161,
            "prop": "name",
            "attributes": {
                "content": "   もちろんだ。",
                "name": "スカルシュレッダー"
            }
        },
        {
            "id": 162,
            "prop": "name",
            "attributes": {
                "content": "   レユニオンがみんなを守るように……俺はお前を守る。",
                "name": "スカルシュレッダー"
            }
        },
        {
            "id": 163,
            "prop": "name",
            "attributes": {
                "content": "   もうお前を傷つけさせない。",
                "name": "スカルシュレッダー"
