        },
        {
            "id": 153,
            "prop": "name",
            "attributes": {
                "content": "   こうして言葉にすることで、自分自身に言い聞かせているんです。",
                "name": "アーミヤ"
            },
            "figure_art": "char_002_amiya_1#7"
        },
