                "focus": 1.0
            }
        },
        {
            "id": 129,
            "prop": "name",
            "attributes": {
                "content": "……",
                "name": "サイレンス"
            },
            "figure_art": "char_108_silent_1#3"
        },
        {
            "id": 130,
