            "attributes": {
                "name": "char_108_silent_1#5",
                "name2": "char_249_muesys_1#2",
                "focus": 2.0
            }
        },
        {
            "id": 40,
            "prop": "name",
            "attributes": {
                "content": "少し度が過ぎたみたいね、ごめんなさい、謝るわ。",
                "name": "ミュルジス"
