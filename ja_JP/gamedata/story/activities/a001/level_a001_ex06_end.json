            "figure_art": "char_003_kalts_1"
        },
        {
            "id": 54,
            "prop": "name",
            "attributes": {
                "content": "   お前とロドスは契約で結ばれている、お前と私も同じだ。",
                "name": "ケルシー"
            },
            "figure_art": "char_003_kalts_1"
        },
