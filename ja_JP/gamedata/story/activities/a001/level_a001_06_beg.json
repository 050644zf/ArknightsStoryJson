        },
        {
            "id": 171,
            "prop": "Character",
            "attributes": {
                "name": "char_263_skadi#2",
                "name2": "char_220_grani#3",
                "focus": 1.0
            }
        },
        {
            "id": 172,
            "prop": "name",
            "attributes": {
                "content": "   他の人を私の……任務に巻き込みたくない。",
                "name": "スカジ"
            }
        },
        {
            "id": 173,
            "prop": "name",
            "attributes": {
                "content": "   今回の目標は、私にとって本当に重要なものだから。",
                "name": "スカジ"
            }
