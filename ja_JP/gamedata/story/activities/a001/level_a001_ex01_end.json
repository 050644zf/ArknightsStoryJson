        {
            "id": 34,
            "prop": "Character",
            "attributes": {
                "name": "char_143_ghost"
            }
        },
        {
            "id": 35,
            "prop": "name",
            "attributes": {
                "content": "   そうですか。ウフフ……素晴らしいことですが、あなたを信じても良いのでしょうか？",
                "name": "スペクター"
            }
        },
        {
            "id": 36,
            "prop": "name",
            "attributes": {
                "content": "   あら。",
                "name": "スペクター"
            }
        },
        {
            "id": 37,
            "prop": "name",
            "attributes": {
