        },
        {
            "id": 91,
            "prop": "Character",
            "attributes": {
                "name": "char_325_bison_1#2",
                "name2": "char_213_mostma_1",
                "focus": 2.0
            }
        },
        {
            "id": 92,
            "prop": "name",
            "attributes": {
                "content": "   いずれ機会はくるさ。でもペンギン急便には期待しない方がいいかもね。私たちは常識で判断してはいけないから。",
                "name": "モスティマ"
            }
        },
        {
            "id": 93,
            "prop": "Character",
            "attributes": {
                "name": "char_325_bison_1#2",
                "name2": "char_213_mostma_1",
                "focus": 1.0
            }
        },
        {
            "id": 94,
            "prop": "name",
            "attributes": {
                "content": "   その点に関しては、もう痛感してます……。",
                "name": "バイソン"
