        {
            "id": 81,
            "prop": "Character",
            "attributes": {
                "name": "char_103_angel_1#7",
                "name2": "char_213_mostma_1",
                "focus": 1.0
            }
        },
        {
            "id": 82,
            "prop": "name",
            "attributes": {
                "content": "   なんか、聞いてるだけで孤独感がすごいね。",
                "name": "エクシア"
            }
        },
        {
            "id": 83,
            "prop": "Character",
            "attributes": {
                "name": "char_103_angel_1#7",
                "name2": "char_213_mostma_1#3",
                "focus": 2.0
            }
        },
        {
            "id": 84,
            "prop": "name",
            "attributes": {
                "content": "   でもこれで旅行記でも書いたら大ヒットするかもしれないよ？",
                "name": "モスティマ"
            }
        },
        {
            "id": 85,
            "prop": "Character",
            "attributes": {
                "name": "char_325_bison_1#2"
            }
        },
        {
            "id": 86,
            "prop": "name",
            "attributes": {
                "content": "   モスティマさんは、この大地にある全ての国に行ったことがあるんですか？",
                "name": "バイソン"
