            }
        },
        {
            "id": 192,
            "prop": "name",
            "attributes": {
                "content": "   はえ？",
                "name": "エクシア"
            },
            "figure_art": "char_103_angel_1#4"
        },
        {
            "id": 193,
            "prop": "Character",
            "attributes": {
                "name": "char_102_texas_1#2"
            }
        },
        {
            "id": 194,
            "prop": "name",
            "attributes": {
                "content": "   みんな、伏せて！",
                "name": "テキサス"
            },
            "figure_art": "char_102_texas_1#2"
        },
        {
