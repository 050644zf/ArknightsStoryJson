        {
            "id": 99,
            "prop": "Character",
            "attributes": {
                "name": "char_325_bison_1#2",
                "name2": "char_103_angel_1#7",
                "focus": 2.0
            }
        },
        {
            "id": 100,
            "prop": "name",
            "attributes": {
                "content": "   そうなの？",
