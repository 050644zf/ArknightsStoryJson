            "prop": "Character",
            "attributes": {
                "name": "avg_npc_182#6"
            }
        },
        {
            "id": 168,
            "prop": "name",
            "attributes": {
                "content": "……はい。",
                "name": "審問官アイリーニ"
            },
            "figure_art": "avg_npc_182#6"
        },
