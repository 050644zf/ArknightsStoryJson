        {
            "id": 145,
            "prop": "name",
            "attributes": {
                "content": "モリー、昨日の夜マイケルさんが会いに行った相手はあんただって言いたいの？",
                "name": "ティッピ"
            },
            "figure_art": "avg_4191_tippi_1#8$1"
