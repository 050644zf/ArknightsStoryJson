        {
            "id": 116,
            "prop": "name",
            "attributes": {
                "content": "全部自分で――",
                "name": "撮影アシスタント"
            },
            "figure_art": "avg_npc_1844_1#6$1"
        },
        {
            "id": 117,
            "prop": "charslot",
