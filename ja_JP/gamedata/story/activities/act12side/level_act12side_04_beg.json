        },
        {
            "id": 76,
            "prop": "name",
            "attributes": {
                "content": "そう心配なさらずとも、大丈夫ですよ。",
                "name": "シデロカ"
            },
            "figure_art": "avg_npc_206_1#8"
        },
        {
            "id": 77,
