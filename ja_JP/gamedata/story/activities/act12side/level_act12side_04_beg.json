        },
        {
            "id": 81,
            "prop": "name",
            "attributes": {
                "content": "どうか、ぜひ安心して楽しんでください。",
                "name": "シデロカ"
            },
            "figure_art": "avg_npc_206_1#8"
        },
        {
            "id": 82,
            "prop": "character",
            "attributes": {
                "name": "avg_npc_190_1#6",
