        {
            "id": 155,
            "prop": "character",
            "attributes": {
                "name": "avg_486_espumo_1#5",
                "name2": "avg_1013_spchen_1#1",
                "focus": 1.0
            }
        },
        {
            "id": 156,
            "prop": "name",
            "attributes": {
                "content": "いやいや、そういうわけじゃないよ。さすがに、ちょっとお喋りしたくらいで味方してもらえるとは思ってないからね。",
